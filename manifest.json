{
	"name": "Quote Verifier",
	"version": "0.0.1",
	"manifest_version": 2,
	"author": "Ryan Rouleau, Jake Crawford",
	"description": "Be informed, verify the quotes you find.",
	"homepage_url": "https://github.com/JakeC1020/quote-verifier-chrome-extension",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_title": "Quote Verifier",
		"default_popup": "src/browser_action/browser_action.html"
	},
	"permissions": [
		"https://quoteverifier.herokuapp.com/api"
	]
}